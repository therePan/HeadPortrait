{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "> 2%", "IE 9", "safari >= 7"]
      },
      "modules": false
    }],
    "es2015-loose",
    "stage-2",
    "react",
    "flow"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    "transform-runtime"
  ]
}
